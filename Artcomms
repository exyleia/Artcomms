  <script>
    const defaultConfig = {
      background_color: "#fff5fb",
      surface_color: "#ffffff",
      text_color: "#9f1239",
      primary_action_color: "#f973b8",
      secondary_action_color: "#f9d976",
      font_family: "system-ui",
      font_size: 14,
      main_title: "Commission Haven for International Artists",
      subtitle: "Temukan & bagikan commission art terbaik di seluruh dunia",
      search_label: "Search for commission",
      other_label: "Other artists",
      share_label: "Share commission",
      footer_text: "Curated commissions for global creatives"
    };

    function applyConfig(config) {
      const cfg = {...defaultConfig, ...config};
      
      const body = document.body;
      const mainEl = document.querySelector('main');
      const mainTitle = document.getElementById('main-title');
      const subtitle = document.getElementById('subtitle-text');
      const searchLabel = document.getElementById('search-label');
      const otherLabel = document.getElementById('other-label');
      const shareLabel = document.getElementById('share-label');
      const footerText = document.getElementById('footer-text');

      // Apply colors
      body.style.backgroundColor = cfg.background_color;
      if (mainEl) {
        mainEl.style.backgroundColor = cfg.surface_color;
        mainEl.style.borderColor = cfg.secondary_action_color;
      }

      // Apply typography
      const base = Number(cfg.font_size) || 14;
      const fontStack = cfg.font_family ? `${cfg.font_family}, system-ui, sans-serif` : 'system-ui, sans-serif';
      document.documentElement.style.fontFamily = fontStack;

      if (mainTitle) {
        mainTitle.textContent = cfg.main_title;
        mainTitle.style.fontSize = `${base * 1.7}px`;
      }
      if (subtitle) {
        subtitle.textContent = cfg.subtitle;
        subtitle.style.fontSize = `${base * 0.95}px`;
        subtitle.style.color = cfg.text_color;
      }
      if (searchLabel) {
        searchLabel.textContent = cfg.search_label;
        searchLabel.style.fontSize = `${base}px`;
        searchLabel.style.color = cfg.text_color;
      }
      if (otherLabel) {
        otherLabel.textContent = cfg.other_label;
        otherLabel.style.fontSize = `${base}px`;
        otherLabel.style.color = cfg.text_color;
      }
      if (shareLabel) {
        shareLabel.textContent = cfg.share_label;
        shareLabel.style.fontSize = `${base}px`;
        shareLabel.style.color = cfg.text_color;
      }
      if (footerText) {
        footerText.textContent = cfg.footer_text;
        footerText.style.fontSize = `${base * 0.8}px`;
        footerText.style.color = cfg.text_color;
      }
    }

    if (window.elementSdk) {
      window.elementSdk.init({
        defaultConfig,
        onConfigChange: async (config) => applyConfig(config),
        mapToCapabilities: (config) => {
          const cfg = {...defaultConfig, ...config};
          return {
            recolorables: [
              {get: () => cfg.background_color, set: (v) => window.elementSdk.setConfig({background_color: v})},
              {get: () => cfg.surface_color, set: (v) => window.elementSdk.setConfig({surface_color: v})},
              {get: () => cfg.text_color, set: (v) => window.elementSdk.setConfig({text_color: v})},
              {get: () => cfg.primary_action_color, set: (v) => window.elementSdk.setConfig({primary_action_color: v})},
              {get: () => cfg.secondary_action_color, set: (v) => window.elementSdk.setConfig({secondary_action_color: v})}
            ],
            borderables: [],
            fontEditable: {get: () => cfg.font_family, set: (v) => window.elementSdk.setConfig({font_family: v})},
            fontSizeable: {get: () => cfg.font_size, set: (v) => window.elementSdk.setConfig({font_size: v})}
          };
        },
        mapToEditPanelValues: (config) => {
          const cfg = {...defaultConfig, ...config};
          return new Map([
            ["main_title", cfg.main_title],
            ["subtitle", cfg.subtitle],
            ["search_label", cfg.search_label],
            ["other_label", cfg.other_label],
            ["share_label", cfg.share_label],
            ["footer_text", cfg.footer_text]
          ]);
        }
      });
      applyConfig(window.elementSdk.config);
    } else {
      applyConfig(defaultConfig);
    }
  </script>
