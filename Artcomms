  <script>
    (function () {
      const defaultConfig = {
        // Colors (max 5)
        background_color: "#fff5fb",   // soft pink background
        surface_color: "#ffffff",      // white card
        text_color: "#9f1239",         // deep rose text
        primary_action_color: "#f973b8", // bright pink
        secondary_action_color: "#f9d976", // soft gold

        // Typography
        font_family: "system-ui",
        font_size: 14,

        // Text content
        main_title: "Commission Haven for International Artists",
        subtitle: "Temukan & bagikan commission art terbaik di seluruh dunia",
        search_label: "Search for commission",
        other_label: "Other artists",
        share_label: "Share commission",
        footer_text: "Curated commissions for global creatives"
      };

      function applyConfig(config) {
        const cfg = Object.assign({}, defaultConfig, config || {});

        const root = document.documentElement;
        const body = document.body;
        const card = document.querySelector(".main-content");
        const iconAvatar = card.querySelector("header div.w-16");
        const iconCircleSearch = document.querySelector("#btn-search span[aria-hidden='true']");
        const iconCircleOther = document.querySelector("#btn-other span[aria-hidden='true']");
        const iconCircleShare = document.querySelector("#btn-share span[aria-hidden='true']");
        const btnSearch = document.getElementById("btn-search");
        const btnOther = document.getElementById("btn-other");
        const btnShare = document.getElementById("btn-share");
        const mainTitle = document.getElementById("main-title");
        const subtitle = document.getElementById("subtitle-text");
        const searchLabel = document.getElementById("search-label");
        const otherLabel = document.getElementById("other-label");
        const shareLabel = document.getElementById("share-label");
        const footerText = document.getElementById("footer-text");

        // Colors
        body.style.backgroundColor = cfg.background_color;
        if (card) {
          card.style.backgroundColor = cfg.surface_color;
          card.style.borderColor = cfg.secondary_action_color;
        }
        if (iconAvatar) {
          iconAvatar.style.backgroundColor = "#ffe9f5";
          iconAvatar.style.borderColor = cfg.secondary_action_color;
        }

        // Buttons base text colors
        [btnSearch, btnOther, btnShare].forEach(function (btn) {
          if (!btn) return;
          btn.style.borderColor = cfg.secondary_action_color;
          btn.style.color = cfg.text_color;
        });

        if (iconCircleSearch) {
          iconCircleSearch.style.backgroundColor = "#ffffff";
          iconCircleSearch.style.color = cfg.primary_action_color;
          iconCircleSearch.style.borderColor = cfg.secondary_action_color;
        }
        if (iconCircleOther) {
          iconCircleOther.style.backgroundColor = "#fff5fb";
          iconCircleOther.style.color = "#d97706";
          iconCircleOther.style.borderColor = cfg.secondary_action_color;
        }
        if (iconCircleShare) {
          iconCircleShare.style.backgroundColor = "#ffffff";
          iconCircleShare.style.color = cfg.primary_action_color;
          iconCircleShare.style.borderColor = cfg.secondary_action_color;
        }

        // Typography scale
        const base = Number(cfg.font_size) || defaultConfig.font_size;
        const baseStack = "system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif";
        const customFont = cfg.font_family || defaultConfig.font_family;
        const fontStack = customFont
          ? customFont + ", " + baseStack
          : baseStack;

        // Apply font family
        if (root) {
          root.style.fontFamily = fontStack;
        }

        if (mainTitle) {
          mainTitle.style.fontSize = (base * 1.7) + "px";
          mainTitle.style.color = "#d97706";
        }
        if (subtitle) {
          subtitle.style.fontSize = (base * 0.95) + "px";
          subtitle.style.color = cfg.text_color;
        }
        if (searchLabel) {
          searchLabel.style.fontSize = base + "px";
          searchLabel.style.color = cfg.text_color;
        }
        if (otherLabel) {
          otherLabel.style.fontSize = base + "px";
          otherLabel.style.color = cfg.text_color;
        }
        if (shareLabel) {
          shareLabel.style.fontSize = base + "px";
          shareLabel.style.color = cfg.text_color;
        }
        if (footerText) {
          footerText.style.fontSize = (base * 0.8) + "px";
          footerText.style.color = cfg.text_color;
        }

        // Text content
        if (mainTitle) {
          mainTitle.textContent = cfg.main_title || defaultConfig.main_title;
        }
        if (subtitle) {
          subtitle.textContent = cfg.subtitle || defaultConfig.subtitle;
        }
        if (searchLabel) {
          searchLabel.textContent = cfg.search_label || defaultConfig.search_label;
        }
        if (otherLabel) {
          otherLabel.textContent = cfg.other_label || defaultConfig.other_label;
        }
        if (shareLabel) {
          shareLabel.textContent = cfg.share_label || defaultConfig.share_label;
        }
        if (footerText) {
          footerText.textContent = cfg.footer_text || defaultConfig.footer_text;
        }
      }

      // Simple inline feedback when buttons clicked
      function setupInteractions() {
        var infoText = document.getElementById("info-text");
        var infoBox = document.getElementById("info-box");
        var btnSearch = document.getElementById("btn-search");
        var btnOther = document.getElementById("btn-other");
        var btnShare = document.getElementById("btn-share");

        function setInfo(message) {
          if (!infoText || !infoBox) return;
          infoText.textContent = message;
          infoBox.classList.add("ring-2", "ring-[#f973b8]");
          setTimeout(function () {
            infoBox.classList.remove("ring-2", "ring-[#f973b8]");
          }, 600);
        }

        if (btnSearch) {
          btnSearch.addEventListener("click", function () {
            setInfo("Mode pencarian: bayangkan halaman untuk mencari artist & slot commission favoritmu.");
          });
        }
        if (btnOther) {
          btnOther.addEventListener("click", function () {
            setInfo("Mode eksplorasi: daftar kreator lain dari berbagai negara untuk kamu cek portofolionya.");
          });
        }
        if (btnShare) {
          btnShare.addEventListener("click", function () {
            setInfo("Mode berbagi: gunakan tautan ini untuk membagikan halaman commission kamu ke klien.");
          });
        }
      }

      // Initialize Element SDK if available
      function initElementSdk() {
        if (!window.elementSdk) {
          applyConfig(defaultConfig);
          setupInteractions();
          return;
        }

        window.elementSdk.init({
          defaultConfig: defaultConfig,
          onConfigChange: async function (config) {
            applyConfig(config || {});
          },
          mapToCapabilities: function (config) {
            var cfg = Object.assign({}, defaultConfig, config || {});

            return {
              recolorables: [
                {
                  get: function () { return cfg.background_color; },
                  set: function (value) {
                    cfg.background_color = value;
                    window.elementSdk.setConfig({ background_color: value });
                  }
                },
                {
                  get: function () { return cfg.surface_color; },
                  set: function (value) {
                    cfg.surface_color = value;
                    window.elementSdk.setConfig({ surface_color: value });
                  }
                },
                {
                  get: function () { return cfg.text_color; },
                  set: function (value) {
                    cfg.text_color = value;
                    window.elementSdk.setConfig({ text_color: value });
                  }
                },
                {
                  get: function () { return cfg.primary_action_color; },
                  set: function (value) {
                    cfg.primary_action_color = value;
                    window.elementSdk.setConfig({ primary_action_color: value });
                  }
                },
                {
                  get: function () { return cfg.secondary_action_color; },
                  set: function (value) {
                    cfg.secondary_action_color = value;
                    window.elementSdk.setConfig({ secondary_action_color: value });
                  }
                }
              ],
              borderables: [],
              fontEditable: {
                get: function () {
                  return cfg.font_family || defaultConfig.font_family;
                },
                set: function (value) {
                  cfg.font_family = value;
                  window.elementSdk.setConfig({ font_family: value });
                }
              },
              fontSizeable: {
                get: function () {
                  return Number(cfg.font_size) || defaultConfig.font_size;
                },
                set: function (value) {
                  cfg.font_size = value;
                  window.elementSdk.setConfig({ font_size: value });
                }
              }
            };
          },
          mapToEditPanelValues: function (config) {
            var cfg = Object.assign({}, defaultConfig, config || {});
            return new Map([
              ["main_title", cfg.main_title],
              ["subtitle", cfg.subtitle],
              ["search_label", cfg.search_label],
              ["other_label", cfg.other_label],
              ["share_label", cfg.share_label],
              ["footer_text", cfg.footer_text]
            ]);
          }
        });

        applyConfig(window.elementSdk.config || defaultConfig);
        setupInteractions();
      }

      initElementSdk();
    })();
  </script>
 <script>(function(){function c(){var b=a.contentDocument||a.contentWindow.document;if(b){var d=b.createElement('script');d.innerHTML="window.__CF$cv$params={r:'9a4f3e78c12ff7e6',t:'MTc2NDIyMTUzNi4wMDAwMDA='};var a=document.createElement('script');a.nonce='';a.src='/cdn-cgi/challenge-platform/scripts/jsd/main.js';document.getElementsByTagName('head')[0].appendChild(a);";b.getElementsByTagName('head')[0].appendChild(d)}}if(document.body){var a=document.createElement('iframe');a.height=1;a.width=1;a.style.position='absolute';a.style.top=0;a.style.left=0;a.style.border='none';a.style.visibility='hidden';document.body.appendChild(a);if('loading'!==document.readyState)c();else if(window.addEventListener)document.addEventListener('DOMContentLoaded',c);else{var e=document.onreadystatechange||function(){};document.onreadystatechange=function(b){e(b);'loading'!==document.readyState&&(document.onreadystatechange=e,c())}}}})();</script></body>
